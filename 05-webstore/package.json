{
  "name": "webstore",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng --aot",
    "ngserve": "ng serve --aot --proxy-config proxy.conf.json --port 4200",
    "server": "nodemon -w ./webstore-api ./webstore-api/server.js",
    "mongo": "mongod --dbpath=c:/mongo-data --port 27017",
    "services": "concurrently --kill-others \"npm run mongo\" \"npm run server\"",
    "start": "npm run ngserve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
