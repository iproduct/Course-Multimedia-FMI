    "@angular/core": "~9.0.5",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "~9.0.5",
    "@angular/material": "~9.2.2",
    "@angular/platform-browser": "~9.0.5",
    "@angular/platform-browser-dynamic": "~9.0.5",
    "@angular/router": "~9.0.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "core-js": "^3.4.1",
    "cors": "^2.8.5",
    "debug": "^3.1.0",
    "express": "^4.16.0",
